#!/usr/bin/env bash

xsetroot -cursor_name left-ptr

keyd-application-mapper -d
picom --config $XDG_CONFIG_HOME/picom/picom.conf &
unclutter --timeout 10 --ignore-scrolling &
feh --no-xinerama --recursive --bg-fill --randomize ~/Pictures/Wallpapers/ &
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &

jack_mixer -c ~/.config/jack_mixer/master.xml &
qpwgraph -m ~/.config/rncbc.org/patchbays/master.qpwgraph &

xmodmap -e 'keycode 135 = Super_R' &
echo xpro loaded
[ -f $XDG_CONFIG_HOME/wacom/config ] && source $XDG_CONFIG_HOME/wacom/config
